{
  "env": {
    "es6": true,
    "jasmine": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "prettier/@typescript-eslint", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "curly": "error",
    "eqeqeq": "error",
    "no-var": "error",
    "prefer-const": "error"
  }
}
