{
  "name": "@jrockwood/package-name",
  "version": "0.1.0",
  "description": "Description of package",
  "repository": "https://github.com/jrockwood/Package.git",
  "author": "Justin Rockwood",
  "license": "MIT",
  "private": true,
  "main": "lib/src/index.js",
  "bin": "lib/src/index.js",
  "scripts": {
    "build": "npm install && npm run clean && npm run compile && npm run lint && npm run test",
    "clean": "rimraf lib",
    "compile": "tsc --project .",
    "compile:watch": "tsc --project . --watch",
    "lint": "npx eslint src/** test/**",
    "lint:fix": "npx eslint src/** test/** --fix",
    "test": "npx cross-env NODE_ENV=test npx jasmine --config=jasmine.json",
    "test:watch": "npx cross-env NODE_ENV=test npx nodemon --ext js --watch lib --exec \"npx jasmine --config=jasmine.json\""
  },
  "devDependencies": {
    "@jrockwood/injected-console-matchers": "2.0.1",
    "@types/fs-extra": "9.0.13",
    "@types/jasmine": "4.0.3",
    "@types/node": "17.0.24",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "cross-env": "7.0.3",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "jasmine": "4.1.0",
    "jasmine-expect": "5.0.0",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@jrockwood/injected-console": "2.0.1",
    "chalk": "4.1.2",
    "fs-extra": "10.0.1",
    "yargs": "17.4.1"
  }
}
